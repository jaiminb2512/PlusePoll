model Vote {
  id           Int          @id @default(autoincrement())

  user         User         @relation(fields: [userId], references: [id])
  userId       Int

  poll         Poll         @relation(fields: [pollId], references: [id])
  pollId       Int

  option       PollOption   @relation(fields: [optionId], references: [id])
  optionId     Int
}
